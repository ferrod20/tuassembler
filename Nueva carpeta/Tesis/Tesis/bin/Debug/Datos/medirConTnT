PATH=$PATH:$HOME/bin

tnt-diff -a -l Entrenado/wsj.lex  WSJ/wsj.gold 	Taggeado/wsj.tagged > Mediciones/wsj.medd
tnt-diff WSJ/wsj.gold 	Taggeado/wsj+NFI.tagged > Mediciones/wsj+NFI.medd

tnt-diff WSJ/wsjComplementoC1.gold 	Taggeado/wsjComplementoC1.tagged > Mediciones/wsjComplementC1.medd
tnt-diff WSJ/wsjComplementoC2.gold 	Taggeado/wsjComplementoC2.tagged > Mediciones/wsjComplementC2.medd
tnt-diff WSJ/wsjComplementoC3.gold 	Taggeado/wsjComplementoC3.tagged > Mediciones/wsjComplementC3.medd
tnt-diff WSJ/wsjComplementoC4.gold 	Taggeado/wsjComplementoC4.tagged > Mediciones/wsjComplementC4.medd

tnt-diff WSJ/wsjComplementoC1.gold 	Taggeado/wsjComplementoC1+NFI.tagged > Mediciones/wsjComplementC1+NFI.medd
tnt-diff WSJ/wsjComplementoC2.gold 	Taggeado/wsjComplementoC2+NFI.tagged > Mediciones/wsjComplementC2+NFI.medd
tnt-diff WSJ/wsjComplementoC3.gold 	Taggeado/wsjComplementoC3+NFI.tagged > Mediciones/wsjComplementC3+NFI.medd
tnt-diff WSJ/wsjComplementoC4.gold 	Taggeado/wsjComplementoC4+NFI.tagged > Mediciones/wsjComplementC4+NFI.medd

tnt-diff WSJ/wsjM1.gold 						Taggeado/wsjM1.tagged > Mediciones/wsjM1.medd
tnt-diff WSJ/wsjM2.gold 						Taggeado/wsjM2.tagged > Mediciones/wsjM2.medd

tnt-diff WSJ/wsjM1.gold 						Taggeado/wsjM1+NFI.tagged > Mediciones/wsjM1+NFI.medd
tnt-diff WSJ/wsjM2.gold 						Taggeado/wsjM2+NFI.tagged > Mediciones/wsjM2+NFI.medd

pause
