PATH=$PATH:$HOME/bin
cd Taggeado
tnt ../Entrenado/wsj ../WSJ/wsj.raw > wsj.tagged
tnt ../Entrenado/wsj+NFI ../WSJ/wsj.raw > wsj+NFI.tagged
tnt ../Entrenado/wsjC1 ../WSJ/wsjComplementoC1.gold > wsjComplementoC1.tagged
tnt ../Entrenado/wsjC2 ../WSJ/wsjComplementoC2.gold > wsjComplementoC2.tagged
tnt ../Entrenado/wsjC3 ../WSJ/wsjComplementoC3.gold > wsjComplementoC3.tagged
tnt ../Entrenado/wsjC4 ../WSJ/wsjComplementoC4.gold > wsjComplementoC4.tagged
tnt ../Entrenado/wsjM1 ../WSJ/wsjM2.gold > wsjM2.tagged
tnt ../Entrenado/wsjM2 ../WSJ/wsjM1.gold > wsjM1.tagged
tnt ../Entrenado/wsjD ../WSJ/wsjComplementoD.gold > wsjComplementoD.tagged

tnt ../Entrenado/wsjC1+NFI ../WSJ/wsjComplementoC1.gold > wsjComplementoC1+NFI.tagged
tnt ../Entrenado/wsjC2+NFI ../WSJ/wsjComplementoC2.gold > wsjComplementoC2+NFI.tagged
tnt ../Entrenado/wsjC3+NFI ../WSJ/wsjComplementoC3.gold > wsjComplementoC3+NFI.tagged
tnt ../Entrenado/wsjC4+NFI ../WSJ/wsjComplementoC4.gold > wsjComplementoC4+NFI.tagged
tnt ../Entrenado/wsjM1+NFI ../WSJ/wsjM2.gold > wsjM2+NFI.tagged
tnt ../Entrenado/wsjM2+NFI ../WSJ/wsjM1.gold > wsjM1+NFI.tagged
tnt ../Entrenado/wsjD+NFI  ../WSJ/wsjComplementoD.gold > wsjComplementoD+NFI.tagged