    <script type="text/javascript" src="../js/compartido/notifications.js" ></script>
    <script type="text/javascript" src="../js/compartido/prompt.js" ></script>        
    <script src="../js/compartido/Api.js" type="text/javascript"></script>
    <script src="../js/compartido/App.js" type="text/javascript"></script>    
    
    <script type="text/javascript" src="../js/home.js"></script>
    
    @*recursos*@
    <script type="text/javascript" src="../js/Recursos/Recurso.js"></script>   
    <script type="text/javascript" src="../js/Recursos/Recurso.plantilla.js"></script>
    <script type="text/javascript" src="../js/Recursos/RecursoController.js"></script>                    
    <script type="text/javascript" src="../js/Recursos/RecursosController.js"></script>     
